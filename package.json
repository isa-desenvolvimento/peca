{
  "name": "peca-rara",
  "private": true,
  "version": "0.4.16",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",
    "prepare": "husky install",
    "commit": "prettier .  --write && git add .  && git-cz",
    "serve": "vite preview --host",
    "start": "node server.js --host",
    "postinstall": "vite build",
    "clear": "rm -Rf node_modules/ && yarn && yarn build && yarn serve"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.6",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "moment": "^2.29.1",
    "mosha-vue-toastify": "^1.0.23",
    "serve-static": "^1.14.2",
    "tw-elements": "^1.0.0-alpha12",
    "v-money3": "^3.21.0",
    "vue": "^3.2.32",
    "vue-avatar-component": "^1.3.1",
    "vue-avatar-editor-improved": "^1.0.7",
    "vue-axios": "^3.4.1",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.14",
    "vue-select": "^4.0.0-beta.3",
    "vue-the-mask": "^0.11.1",
    "vue3-carousel": "^0.1.36",
    "vue3-markdown-it": "^1.0.10",
    "vue3-popper": "^1.4.2",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@vitejs/plugin-vue": "^2.2.4",
    "autoprefixer": "^10.4.4",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "tailwindcss": "^3.0.23",
    "vite": "^2.8.0",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-pwa": "^0.11.13"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
